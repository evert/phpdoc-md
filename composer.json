{
    "name": "evert/phpdoc-md",
    "description" : "PHP documentation generator, with markdown output",
    "keywords" : [ "PHP", "Markdown", "PHPDoc" ],
    "homepage" : "https://github.com/evert/phpdoc-md",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.4.0",
        "twig/twig" : "~1.2|2.0"
    },
    "require-dev" : {
        "phpdocumentor/phpdocumentor" : "~2.8.0",
        "sabre/cs"                    : "~0.0.5"
    },
    "authors" : [
        {
            "name" : "Evert Pot",
            "email" : "evert@rooftopsolutions.nl",
            "homepage" : "http://www.rooftopsolutions.nl/",
            "role" : "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "PHPDocMD\\" : "src/"
        }
    },
    "bin" : [
        "bin/phpdocmd"
    ],
    "config" : {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
